<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:spring="http://www.springframework.org/schema/beans"
	xmlns:cxf="http://www.mulesource.org/schema/mule/cxf/2.2" xmlns:https="http://www.mulesource.org/schema/mule/https/2.2"
	xsi:schemaLocation="
               http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
               http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
               http://www.mulesource.org/schema/mule/https/2.2 http://www.mulesource.org/schema/mule/https/2.2/mule-https.xsd
               http://www.mulesource.org/schema/mule/cxf/2.2  http://www.mulesource.org/schema/mule/cxf/2.2/mule-cxf.xsd">

	<!-- Name of this file is mandatory and may not be changed. -->
<model name="Bridged-ApotekenService-SkrivUtDosRecept-model">

		<service
			name="Bridged-ApotekenService-SkrivUtDosRecept-service">
			<inbound>
				<cxf:inbound-endpoint
							address="${inbound.endpoint.apotekensservice.or.SkrivUtDosRecept}"
							wsdlLocation="classpath:/schemas/interactions/SkrivUtDosReceptInteraction/SkrivUtDosReceptInteraction_1.0_rivtabp20.wsdl"
							serviceName="SkrivUtDosReceptResponderService" namespace="urn:riv:inera:se.apotekensservice:or:SkrivUtDosRecept:1:rivtabp20"
							proxy="true" payload="envelope" synchronous="true" transformer-refs="ArgosHeaderToSamlTicket" applyTransformersToProtocol="false"/>
			</inbound>
			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="${outbound.endpoint.apotekensservice.or.SkrivUtDosRecept}"
						synchronous="true" proxy="true" payload="envelope"
						transformer-refs="addContentTypeXmlUtf8" responseTransformer-refs="IgnoreExceptionPayloads"/>
				</pass-through-router>
			</outbound>
		</service>
		
		<service
			name="Bridged-ApotekenService-SkrivUtDosRecept-service-env2">
			<inbound>
				<cxf:inbound-endpoint
							address="${inbound.endpoint.env2.apotekensservice.or.SkrivUtDosRecept}"
							wsdlLocation="classpath:/schemas/interactions/SkrivUtDosReceptInteraction/SkrivUtDosReceptInteraction_1.0_rivtabp20.wsdl"
							serviceName="SkrivUtDosReceptResponderService" namespace="urn:riv:inera:se.apotekensservice:or:SkrivUtDosRecept:1:rivtabp20"
							proxy="true" payload="envelope" synchronous="true" transformer-refs="ArgosHeaderToSamlTicket" applyTransformersToProtocol="false"/>
			</inbound>
			<outbound>
				<pass-through-router>
					<cxf:outbound-endpoint
						address="${outbound.endpoint.env2.apotekensservice.or.SkrivUtDosRecept}"
						synchronous="true" proxy="true" payload="envelope"
						transformer-refs="addContentTypeXmlUtf8" responseTransformer-refs="IgnoreExceptionPayloads"/>
				</pass-through-router>
			</outbound>
		</service>
	</model>
</mule>
