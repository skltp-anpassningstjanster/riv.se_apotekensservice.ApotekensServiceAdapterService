                        <xs:documentation>Lista med AFF-fel</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="affStatus" type="xs:int" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Status för hela ordinationen eller uttaget. Allvarligaste AFF-felet styr status för hela ordinationen.

0 = Accepterad, 1 = Accepterad med varning, 2 = Avvisad.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="kontrolltidpunkt" type="xs:dateTime" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Datum och tidpunkt för utförd AFF-kontroll.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="kontrolltyp" type="xs:int" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Typ av AFF-kontroll som har genomförts.

1 = AFF-PRENEF
2 = AFF-NEF
3 = AFF-REG
4 = AFF-KORR
5 = AFF-EXP
6 = AFF-KEX
7 = AFF-MAK
8 = AFF-NOD
9 = AFF-UTS</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
            </xs:sequence>
        </xs:complexType>
    
    
                
