    <xs:complexType name="OrdinationsinformationResponse">
        <xs:sequence>
            <xs:element name="apotekspersonal" type="ApotekspersonalResponse" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Den som godkänt ordinationen.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="lagringstidpunkt" type="xs:dateTime" nillable="false">
                <xs:annotation>
                    <xs:documentation>Datum och tid då ordinationen skapades i OR.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="mottagandeApotek" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Apoteks-id (GLN) vid direktadressering av ordinationer.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="ordinationstidpunkt" type="xs:dateTime" nillable="false">
                <xs:annotation>
                    <xs:documentation>Datum och eventuell tid för ordinationens utfärdande.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="ordinator" type="or4:OrdinatorResponse" nillable="false">
                <xs:annotation>
                    <xs:documentation>Den ordinatör som utfärdat ordinationen.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="originalformat" type="xs:int" nillable="false">
                <xs:annotation>
                    <xs:documentation>Originalhandlingens ursprung.

                        0 = Pappersordination
                        1 = Telefonordination
                        2 = E-recept (NEF)
                        (3 = Faxordination patient)
                        4 = Faxordination
                        5 = E-ordination
                        6 = Utskrivet från OR
                        7 = Utskriven E-ordination.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="originalordinationsId" type="xs:string" nillable="false">
                <xs:annotation>
                    <xs:documentation>Unikt id för en ordination. Dock ej unikt för e-recept som tillhör receptsamling.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="patient" type="or4:PatientResponse" nillable="false">
                <xs:annotation>
                    <xs:documentation>Den patient som ordinationen gäller för.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="registrerandeApotekspersonal" type="or4:RegistrerandeApotekspersonalResponse" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Den som registrerat ordinationen.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="testindikator" type="xs:int" nillable="false">
                <xs:annotation>
                    <xs:documentation>Markering om ordinationen är äkta,
                        för test eller för utbildning.

                        1 = Äkta
                        2 = Test
                        3 = Utbildning</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="extension" type="ExtensionType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ForandringResponse">
