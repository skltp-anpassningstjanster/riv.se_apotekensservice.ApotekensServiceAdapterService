            </xs:element>

            <xs:element name="apoteketsKommentar" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Farmaceutens kommentar på ordinationen.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="artikelinformation" type="or4:ArtikelinformationResponse" nillable="false">
                <xs:annotation>
                    <xs:documentation>Information om artikeln som ordinerats.</xs:documentation>
