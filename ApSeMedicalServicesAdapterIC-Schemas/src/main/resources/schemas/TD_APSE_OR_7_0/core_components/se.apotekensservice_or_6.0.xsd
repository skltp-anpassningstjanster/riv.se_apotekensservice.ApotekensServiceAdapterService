            </xs:element>

            <xs:element name="behandlingsinformation" type="or4:BehandlingsinformationResponse" nillable="false" />

            <xs:element name="dispenseringsregler" type="or4:DispenseringsreglerResponse" nillable="false">
                <xs:annotation>
