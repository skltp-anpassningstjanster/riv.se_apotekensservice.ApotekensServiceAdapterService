        <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="23"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="PeriodlangdType">
        <xs:annotation><xs:documentation>Antal dagar som dosering skall gälla.
            Vid regelbunden dosering anges periodlängd = 1
            Vid oregelbunden dosering anges antal dagar som intervallet omfattar.
            Exempelvis 2 om intag ska ske varannan dag.</xs:documentation></xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="28"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PersonnummerType">
        <xs:annotation>
            <xs:documentation>Giltigt personnummer.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="12"/>
            <xs:maxLength value="12"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="KonType">
        <xs:annotation>
            <xs:documentation>Patientens kön.

