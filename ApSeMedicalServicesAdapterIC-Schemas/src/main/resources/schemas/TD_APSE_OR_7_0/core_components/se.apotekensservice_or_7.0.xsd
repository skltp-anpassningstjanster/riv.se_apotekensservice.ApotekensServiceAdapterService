                <itemizedlist>
                    <listitem>1 = Man</listitem>
                    <listitem>2 = Kvinna</listitem>
                    <listitem>3 = Ej specificerat</listitem>
                </itemizedlist>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="3"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="AdressType">
        <xs:annotation><xs:documentation>Adress</xs:documentation></xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PostnummerType">
        <xs:annotation>
            <xs:documentation>Postnummer</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="PostortType">
        <xs:annotation>
            <xs:documentation>Postort</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="28"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TelefonnummerBasType">
        <xs:annotation>
            <xs:documentation>Telefonnummer</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TelefonnummerType">
        <xs:annotation>
            <xs:documentation>Patientens eller förskrivarens telefonnummer 1. Obligatorisk för förskrivare.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="TelefonnummerBasType">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TestindikatorType">
        <xs:annotation>
            <xs:documentation>Markering om ordinationen är äkta,
                för test eller för utbildning.

                1 = Äkta
                2 = Test
                3 = Utbildning
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>

    <xs:simpleType name="OrdinationstypType">
        <xs:annotation>
            <xs:documentation>Typ av ordination.
                Anges enbart på ordinationer till dospatient.

                B = Helförpackning vid behov
                S = Helförpackning stående
                D = Dispenserad ordination
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>
    </xs:simpleType>

    <xs:simpleType name="KommentarType">
        <xs:annotation><xs:documentation>Kommentar vid förändring, utsättning eller makulering av ordination.
            Obligatorisk parameter utom vid makulering med orsakskod annan än 4.</xs:documentation></xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="254"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AntalUttagType">
        <xs:annotation>
            <xs:documentation>Antal tillåtna uttag på ordinationen.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="4"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ExpeditionsintervallType">
        <xs:annotation>
            <xs:documentation>Absolut expeditionsintervall. Av förskrivaren angiven kortaste tid mellan två
                expeditioner.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AntalForpackningarType">
        <xs:annotation>
            <xs:documentation>Antal förpackningar per uttag</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1"/>
            <xs:totalDigits value="4"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BytesEjType">
        <xs:annotation>
            <xs:documentation>Markering att farmaceut eller patient beslutat att ordinerad artikel ej får bytas ut.

                A=Apoteks-nej, L=Förskrivar-nej, P=Patient-nej.

                Får enbart anges för Ordinationstyp=D (Dispenserad ordination).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>
    </xs:simpleType>

    <xs:simpleType name="OriginalformatType">
        <xs:annotation>
            <xs:documentation>Originalhandlingens ursprung.
                0 = Pappersordination
                1 = Telefonordination
                2 = E-recept (NEF)
                (3 = Faxordination patient)
                4 = Faxordination
                5 = E-ordination
                6 = Utskrivet från OR
                7 = Utskriven E-ordination</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="PappersOriginalformatType">
        <xs:annotation>
            <xs:documentation>Originalhandlingens ursprung.
                0 = Pappersordination
                1 = Telefonordination
                4 = Faxordination
                6 = Utskrivet från OR
                7 = Utskriven E-ordination</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>

    <xs:simpleType name="ApoteketsKommentarType">
        <xs:annotation>
            <xs:documentation>Farmaceutens kommentar på ordinationen.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="512"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BefattningskodType">
        <xs:annotation>
            <xs:documentation>Användarens befattningskod enligt HSA.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ForskrivarkodType">
        <xs:annotation>
            <xs:documentation>Förskrivarens individuella legitimationskod eller förskrivarens gruppförskrivarkod då individuell kod saknas.
                 Gruppförskrivarkod används då förskrivaren saknar individuell förskrivarkod men har via sin roll erhållit förskrivningsrätt. Kan exempelvis vara en AT-läkare eller förskrivare med förordnande.
                 Förskrivarkoden definieras av Socialstyrelsen och gruppförskrivarkoden definieras av eHälsomyndigheten.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="7"/>
            <xs:maxLength value="7"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="LegitimationskodType">
        <xs:annotation>
            <xs:documentation>Förskrivares sjuställiga förskrivarkod alternativt legitimerad användares sexställiga
                legitimationskod enligt Socialstyrelsen. Obligatorisk för gruppförskrivare (ej läkare) och för farmaceuter.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="6"/>
            <xs:maxLength value="7"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AktorsExpeditionsIdType">
        <xs:annotation>
            <xs:documentation>Expedierande apoteksaktörs expeditions-id.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="12"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ExpeditionsIdType">
        <xs:annotation>
            <xs:documentation>eHälsomyndighetens expeditions-id.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:totalDigits value="16"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ExpeditionsradIdType">
        <xs:annotation>
            <xs:documentation>Expeditionsrad-id som anges för expeditionen.
                En expeditionsrad är unik tillsammans med
                antingen Apoteksaktörens expeditions-id eller
                eHälsomyndighetens expeditions-id.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="ExpeditionsIdType"/>
    </xs:simpleType>

    <xs:simpleType name="FarmaceutsNoteringType">
        <xs:annotation>
            <xs:documentation>Farmaceuts notering angående uttaget.Valideras med EnkelTextValidator.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="254"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="UnderlagsversionType">
        <xs:annotation>
            <xs:documentation>Version på Ordinationslista. Används för att verifiera att klientsystemet har rätt underlag vid uppdateringar.
                Obligatorisk parameter om ordinationen tillhör patient med dosunderlag.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="8"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AnvandareIdType">
        <xs:annotation>
            <xs:documentation>Unikt användarid i anropande system.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AnvandareType">
        <xs:annotation>
            <xs:documentation>Unikt användarid i anropande system.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="AnvandareIdType">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="RadnummerNEFType">
        <xs:annotation>
            <xs:documentation>Unikt id för en rad i en receptsamling som skickas in via NEF. Radnumret är en kombination av rad inom receptsamlingen och eventuellt flerval. Id:t sätts av vårdsystemet.

                Radnummer utan flerval:  1,2,...n
                Radnummer med flerval: 1-1, 1-2, 1-n</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="5"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="OrdinationsStatusType">
        <xs:annotation>
            <xs:documentation>Status för dosunderlag eller ordination.
                202 = Nytt
                203 = Korrigerat
                205 = Expedierbart
                207 = Slutexpedierat
                209 = Utskrivet
                210 = Utgånget
                211 = Historik
                212 = Makulerat
                214 = Avvisat
                215 = Avliden
                216 = Spärrat
                217 = Makulerat av förskrivare
                218 = Utsatt av förskrivare
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="3"/>
        </xs:restriction>

    </xs:simpleType>

    <xs:simpleType name="UttagsstatusType">
        <xs:annotation>
            <xs:documentation>Status uttag.
                200 = Godkänt
                210 = Borttaget
                300 = Fiktivt
                400 = Pappersrecept
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="3"/>
        </xs:restriction>

    </xs:simpleType>
    <xs:simpleType name="RadversionType">
        <xs:annotation>
            <xs:documentation>Ordinationens versionsnummer för intern versionshantering av en ordination. Ny
                radversion av ordinationen skapas vid varje uttag.
                Returneras alltid men kan vara utan värde.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>

    <xs:simpleType name="AllvarlighetsgradType">
        <xs:annotation>
            <xs:documentation>Kod för att beskriva hur allvarligt ett AFF-fel är.

                1 = Accepterad med varning, 2 = Avvisad.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>

    <xs:simpleType name="AffFelkodType">
        <xs:annotation>
            <xs:documentation>Felkod enligt Automatisk format- och författningskontroll (AFF).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="5"/>
            <xs:maxLength value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AffFelmeddelandeType">
        <xs:annotation>
            <xs:documentation>Felmeddelande för AFF-fel.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="254"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MaxDygnsdosType">
        <xs:annotation>
            <xs:documentation>Maximal mängd läkemedel som får förbrukas per dygn enligt doseringsschema eller
                doseringsanvisning.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="UttagsIdType">
        <xs:annotation>
            <xs:documentation>Unikt id för uttaget</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:totalDigits value="16"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ExpedieradMangdType">
        <xs:annotation>
            <xs:documentation>Totalmängd för uttaget. Anges vid dispenserat uttag och vid helförpackningsuttag
                med bruten förpackning, beräknas annars utifrån antal expedierade förpackningar.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="ExpedieratAntalForpackningarType">
        <xs:annotation>
            <xs:documentation>Antal expedierade förpackningar i detta uttag.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="1"/>
            <xs:totalDigits value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExpedieratAntalUttagType">
        <xs:annotation>
            <xs:documentation>Förändring av receptets antal uttag genom detta uttag</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="HistorikSkapandeTypType">
        <xs:annotation>
            <xs:documentation>Anger vilken typ som avses. M =Makulering, U = Utsättning, K = Korrigerad, S =
                Spärrad, O = Utskrivet original.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>
    </xs:simpleType>

    <xs:simpleType name="OrdinationKortInfoStatusType">
        <xs:annotation>
            <xs:documentation>Ordinationens tillståndsstatus.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MakuleringOrsakskodType">
        <xs:annotation>
            <xs:documentation>Orsakskod som strukturerat beskriver orsak till makulering.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MakuleringPaUppdragAvType">
        <xs:annotation>
            <xs:documentation>Uppgift om vem som har gett i uppdrag att genomföra makulering. P = Patient, F =
                Förskrivare.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>
    </xs:simpleType>
    <xs:simpleType name="MakuleringOrsakskodKlartextType">
        <xs:annotation>
            <xs:documentation>Orsakskoder vid makulering: 1 = Fel patient. 2 = Fel läkemedel/vara/
                styrka/ändamål. Patienten informerad. 3 = Utsatt lm/Inaktuell medicinering. Patienten
                informerad. Gäller ej dospatienter då utsättning skall användas. 4 = Annan orsak. Patienten
                informerad.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArbetsplatsType">
        <xs:annotation>
            <xs:documentation>Unik kod för identifiering av administratörens arbetsplats.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="13"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AktorsnamnType">
        <xs:annotation>
            <xs:documentation>Officiellt namn för aktör</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="BesoksadressType" >
        <xs:annotation>
            <xs:documentation>Besöksadress</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FaxType">
        <xs:annotation>
            <xs:documentation>Faxnummer</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="GlnKodType">
        <xs:annotation>
            <xs:documentation>GLN-kod</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="13"/>
            <xs:maxLength value="13"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExpeditionsstalleHuvudtypkodType">
        <xs:annotation>
            <xs:documentation>Kod som beskriver huvudverksamheten för expeditionsstället</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExpeditionsstalleNamnType">
        <xs:annotation>
            <xs:documentation>Expeditionsställets officiella namn</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ExpeditionsstalleOrtType">
        <xs:annotation>
            <xs:documentation>Expeditionsställets besöksort</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArbetsplatsNamnType">
        <xs:annotation>
            <xs:documentation>ArbetsplatsNamn</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArbetsplatsOrtType">
        <xs:annotation>
            <xs:documentation>ArbetsplatsOrt</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PostadressType">
        <xs:annotation>
            <xs:documentation>Postadress</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AlternativtAntalKlartextType">
        <xs:annotation>
            <xs:documentation>Sammanslagning av multiplar, antal och alternativ enhet. Om alternativ enhet
                saknas för artikeln är detta fält tomt.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AntalKlartextType">
        <xs:annotation>
            <xs:documentation>Sammanslagning av multiplar, antal och enhet på storleken.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ArtikelbenamningType">
        <xs:annotation>
            <xs:documentation>Sammanfattande beskrivning på en artikel (läkemedelsnamn + läkemedelsform + styrka
                + styrkans enhet + företag vid behov)
                Förkortningar kan förekomma.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AtcKlartextType">
        <xs:annotation>
            <xs:documentation>ATC-kod i klartext enligt lexikontabell. Språk anges på svenska (sv).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="240"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AtcKodType">
        <xs:annotation>
            <xs:documentation>ATC-kod</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ForpackningsenhetType">
        <xs:annotation>
            <xs:documentation>Enhet på förpackningsstorleken, i klartext. Returnerar den alternativa enheten om
                sådan finns angiven för artikeln. Språk anges
                på svenska (sv).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ForpackningsinnehallType">
        <xs:annotation>
            <xs:documentation>Beskrivning av förpackning, innehåll och antal.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="160"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ForpackningsmangdType">
        <xs:annotation>
            <xs:documentation>Totala mängden i förpackningen. Returnerar den alternativa förpackningsmängden om
                sådan finns angiven för artikeln.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="ForpackningstypType">
        <xs:annotation>
            <xs:documentation>Beskrivning av förpackningstyp för en artikel.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IntressentType">
        <xs:annotation>
            <xs:documentation>Intressent (företag) som är ansvarig för artikeln.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="160"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NarkotikaklassType">
        <xs:annotation>
            <xs:documentation>Koden för narkotikaklass enligt Läkemedelsverkets förteckning:

                - = Ospecificerad

                0 = Ej narkotikaklassad

                1 = II

                2 = Narkotika förteckning IV/V

                3 = III

                4 = IV

                5 = V

                6 = I
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NarkotikaklassKlartextType">
        <xs:annotation>
            <xs:documentation>Narkotikaklass i klartext enligt Läkemedelsverket förteckning. Klartext enligt
                lexikontabell för narkotikaklass. Språk anges på
                svenska (sv).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="90"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ProdukttypType">
        <xs:annotation>
            <xs:documentation>Artikelns produkttyp som kod.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ProdukttypKlartextType">
        <xs:annotation>
            <xs:documentation>Artikelns produkttyp som klartext enligt lexikontabell för produkttyp. Språk anges
                på svenska (sv).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SarskiltLakemedelKodType">
        <xs:annotation>
            <xs:documentation>Kod som anger om särskild receptblankett krävs och om recept får itereras.

                1 = Särskilt receptblankett krävs INTE.

                2 = Särskild receptblankett krävs. Recepten får itereras.

                3 = Särskild receptblankett krävs. Recepten får EJ itereras.

                4 = Särskild receptblankett krävs. Recept för humant bruk får itereras.

                - = Ospecifierat
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DispenseringsreglerBytesEjType">
        <xs:annotation>
            <xs:documentation>Markering att farmaceut eller patient beslutat att ordinerad artikel ej får bytas ut.

                A=Apoteks-nej, P=Patient-nej.

                Får enbart anges för Ordinationstyp=D (Dispenserad ordination).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>
    </xs:simpleType>
    <xs:simpleType name="DosunderlagsversionType">
        <xs:annotation>
            <xs:documentation>Ny Dosunderlagsversion skapas varje gång Dosunderlaget ändrar status till "Ej
                godkänt".
                Dosunderlagsversionen skrivs ut på dosreceptet.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="6"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DosunderlagsstatusType">
        <xs:annotation>
            <xs:documentation>Status för dosunderlag
                500 = Ej godkänt
                510 = Godkänt
                511 = Tidigare godkänt
                515 = Avliden
                520 = Avregistrerad
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="GruppforskrivarkodKlartextType">
        <xs:annotation>
            <xs:documentation>Gruppförskrivarkod klartext.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ResterandeAntalUttagType">
        <xs:annotation>
            <xs:documentation>Kvarvarande antal uttag på ordinationen</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:totalDigits value="4"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MaxVeckodosType">
        <xs:annotation>
            <xs:documentation>Maximal mängd läkemedel som får förbrukas per vecka enligt doseringsschema eller
                doseringsanvisning.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="TotalmangdType">
        <xs:annotation>
            <xs:documentation>Förskriven totalmängd.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="TotalmangdKvarType">
        <xs:annotation>
            <xs:documentation>Resterande mängd som finns kvar på receptet</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:double"/>
    </xs:simpleType>

    <xs:simpleType name="UtfardarkategoriType">
        <xs:annotation>
            <xs:documentation>Förskrivarens utfärdarkategori enligt NEF.

                LAK = Läkare
                ATL = AT-läkare
                LMF = Läkare med förordnande
                TAN = Tandläkare
                VET = Veterinär
                TAH = Tandhygienist
                BMO = Barnmorska
                SSK = Sjuksköterska
                HJM = Hjälpmedelsförskrivare
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="YrkesKodKlartextType">
        <xs:annotation>
            <xs:documentation>Yrkeskod för utfärdarkategori, klartext.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SpecialistKodKlartextType">
        <xs:annotation>
            <xs:documentation>Lista med specialistkod för utfärdarkategori, klartext.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ForskrivningsratterType">
        <xs:annotation>
            <xs:documentation>Lista med förskrivningsrätter för artikeln. Använder koder enligt NPL.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AvvisningType">
        <xs:annotation>
            <xs:documentation>Orsak till avvisning, 80 tecken.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ResultatMeddelande">
        <xs:annotation>
            <xs:documentation>Resultatmeddelande.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="254"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
