    <!-- Standard definitioner Types -->
    <xs:simpleType name="OrdinationsIdType">
        <xs:annotation>
            <xs:documentation>Ordinationsid</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="OriginalordinationsIdUuidType">
        <xs:annotation>
            <xs:documentation>Unikt id för en ordination. Dock ej unikt för e-recept som tillhör
                receptsamling.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="41"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="EfternamnType">
        <xs:annotation>
            <xs:documentation>Efternamn, 35 tecken.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="FornamnType">
        <xs:annotation>
            <xs:documentation>Förnamn, 35 tecken.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="YrkeskodType">
        <xs:annotation>
            <xs:documentation>
                Legitimerade yrkeskoder och utökade yrkeskoder
                AP = Apotekare
                BM = Barnmorska
                LK = Läkare
                RC = Receptarie
                SJ = Sjuksköterska
                TH = Tandhygienist
                TL = Tandläkare
                AE = Apotekselev
                TE = Apotekstekniker
                RE = Receptarie elev
                AL = AT-läkare
                LF = Läkare med förordnande
                VP = Vårdpersonal
                VT = Veterinär
                FA = Europeisk farmaceut
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AffKontrolltypType">
        <xs:annotation>
            <xs:documentation>Typ av AFF-kontroll som har genomförts.
                1 = AFF-PRENEF
                2 = AFF-NEF
                3 = AFF-REG
                4 = AFF-KORR
                5 = AFF-EXP
                6 = AFF-KEX
                7 = AFF-MAK
                8 = AFF-NOD
                9 = AFF-UTS
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>

    <xs:simpleType name="AffStatusType">
        <xs:annotation>
            <xs:documentation>Status för hela ordinationen eller uttaget. Allvarligaste AFF-felet styr status för hela
                ordinationen.
                0 = Accepterad
                1 = Accepterad med varning
                2 = Avvisad
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>

    </xs:simpleType>

    <xs:simpleType name="OrgNrType">
        <xs:annotation>
            <xs:documentation>Organisationsnummer.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:totalDigits value="10"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NplIdType">
        <xs:annotation>
            <xs:documentation>Npl-id.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="14"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NplPackIdType">
        <xs:annotation>
            <xs:documentation>Nplpack-id.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="NplIdType"/>
    </xs:simpleType>

    <xs:simpleType name="VaruNrType">
        <xs:annotation>
            <xs:documentation>Nordiskt varunummer.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="6"/>
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="FormansvalType">
        <xs:annotation>
            <xs:documentation>Förmånsval som utgjorde underlag för expeditionen.
                R = Med förmån
                U = Utan förmån
                L = Livsmedel
                F = Kostnadsfritt
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>

    </xs:simpleType>

    <xs:simpleType name="UttagstypType">
        <xs:annotation>
            <xs:documentation>Parameter som visar vilken typ av uttag som avses.
                Värdemängd: D = Dispenserat uttag,
                H = Helförpackningsuttag.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="char"/>
    </xs:simpleType>

    <xs:simpleType name="DoseringstextType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="256"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AndamalKlartextType">
        <xs:restriction base="xs:string">
            <xs:maxLength value="256"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SprakkodType">
        <xs:annotation><xs:documentation>Främmande språkkod/landskod enligt ISO-639-1</xs:documentation></xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="char">
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="IntervallenhetType">
        <xs:annotation>
            <xs:documentation>Enhet för det absoluta expeditionsintervallet. Kan anges i dagar, veckor eller
                månader.

                Mn = Månader
                V = Veckor
                Dg = Dagar

                Obligatorisk parameter om Expeditionsintervall anges.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>

    <xs:simpleType name="ArbetsplatskodType">
        <xs:annotation>
            <xs:documentation>Unik kod för identifiering av förskrivarens
                arbetsplats vid ordinationstillfället.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="3"/>
            <xs:maxLength value="13"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="AltArbetsplatskodType">
        <xs:annotation>
            <xs:documentation>Unik kod för identifiering av förskrivarens
                arbetsplats vid ordinationstillfället.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>

