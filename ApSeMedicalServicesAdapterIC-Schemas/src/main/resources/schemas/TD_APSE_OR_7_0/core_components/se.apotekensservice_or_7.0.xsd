    <xs:complexType name="AFFFelResponse">
        <xs:sequence>
            <xs:element name="allvarlighetsgrad" type="AllvarlighetsgradType" nillable="false">
                <xs:annotation>
                    <xs:documentation>Kod för att beskriva hur allvarligt ett AFF-fel är.

                        1 = Accepterad med varning, 2 = Avvisad.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="felkod" type="AffFelkodType" nillable="false">
                <xs:annotation>
                    <xs:documentation>Felkod enligt Automatisk format- och författningskontroll (AFF).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="felmeddelande" type="AffFelmeddelandeType" nillable="false">
                <xs:annotation>
                    <xs:documentation>Felmeddelande för AFF-fel.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="extension" type="ExtensionType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="AFFResultatResponse">
        <xs:sequence>
            <xs:element name="affFelLista" type="AFFFelResponse" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Lista med AFF-fel</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="affStatus" type="AffStatusType" nillable="false">
                <xs:annotation>
                    <xs:documentation>Status för hela ordinationen eller uttaget. Allvarligaste AFF-felet styr status
                        för hela ordinationen.

                        0 = Accepterad, 1 = Accepterad med varning, 2 = Avvisad.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="avvisningsorsak" type="AvvisningType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Information om avvisningsorsak.</xs:documentation>
                </xs:annotation>
            </xs:element>


            <xs:element name="kontrolltidpunkt" type="xs:dateTime" nillable="false">
                <xs:annotation>
                    <xs:documentation>Datum och tidpunkt för utförd AFF-kontroll.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="kontrolltyp" type="AffKontrolltypType" nillable="false">
                <xs:annotation>
                    <xs:documentation>Typ av AFF-kontroll som har genomförts.
                        1 = AFF-PRENEF
                        2 = AFF-NEF
                        3 = AFF-REG
                        4 = AFF-KORR
                        5 = AFF-EXP
                        6 = AFF-KEX
                        7 = AFF-MAK
                        8 = AFF-NOD
                        9 = AFF-UTS
