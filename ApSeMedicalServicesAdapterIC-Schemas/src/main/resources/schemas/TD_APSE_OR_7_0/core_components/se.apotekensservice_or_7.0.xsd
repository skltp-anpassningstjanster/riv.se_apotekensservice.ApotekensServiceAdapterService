            <xs:maxInclusive value="28"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="IntagsmangdType">
        <xs:annotation><xs:documentation>Intagsmängd per tillfälle.</xs:documentation></xs:annotation>
        <xs:restriction base="xs:double">
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="IntagstillfalleType">
